{
  "homepage": "https://ItsYahBoilaurence.github.io/wellbe-frontend-repo",
  "name": "mobile-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "dev": "vite --port 3400",
    "build": "vite build",
    "preview": "vite preview",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json}\" --ignore-path .prettierignore",
    "format:fix": "prettier --write \"**/*.{js,jsx,ts,tsx,json}\" --fix --ignore-path .prettierignore",
    "lint": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --fix --ignore-path ./.eslintignore && tsc --noEmit",
    "lint:fix": "eslint . --ext js,jsx,ts,tsx --cache --ignore-path .eslintignore --fix && tsc --noEmit"
  },
  "dependencies": {
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.4",
    "@emotion/serialize": "^1.1.4",
    "@emotion/utils": "^1.2.1",
    "@mantine/carousel": "^7.10.1",
    "@mantine/core": "^7.10.1",
    "@mantine/dropzone": "^7.10.1",
    "@mantine/emotion": "^7.10.1",
    "@mantine/hooks": "^7.10.1",
    "@mantine/modals": "^7.10.1",
    "@mantine/notifications": "^7.10.1",
    "@mantine/nprogress": "^7.10.1",
    "@tabler/core": "^1.0.0-beta20",
    "@tabler/icons-react": "^3.12.0",
    "@tanstack/react-query": "^5.51.23",
    "aws-amplify": "5.3.12",
    "axios": "^1.7.9",
    "embla-carousel-autoplay": "^8.2.0",
    "firebase": "^11.1.0",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.52.2",
    "react-router-dom": "^6.26.0",
    "universal-cookie": "^7.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "gh-pages": "^6.3.0",
    "typescript": "^5.7.3",
    "vite": "^5.4.11"
  }
